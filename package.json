{
  "name": "wdown",
  "version": "1.0.0",
  "description": "yet another markdown editor",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-main": "cross-env NODE_ENV=producton webpack --config configs/webpack.config.main.prod.babel.ts",
    "build-renderer": "cross-env NODE_ENV=producton webpack --config configs/webpack.config.renderer.prod.babel.ts",
    "dev": "yarn start-renderer-dev",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.ts",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./src/main.babel.js"
  },
  "main": "index.js",
  "author": "natsuki <yukihiratype2@live.com>",
  "license": "MIT",
  "build": {
    "productName": "ElectronReactTypescript",
    "appId": "org.natsuki.ElectronReactTypescript",
    "app": [
      "output/",
      "package.json"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.7.4",
    "@types/node": "^12.12.12",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "3",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react": "^7.16.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "scss-loader": "^0.0.1",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "file-loader": "^4.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18"
  }
}
